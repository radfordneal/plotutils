#!/bin/bash

# Run checks and timing tests.

TOP=`git rev-parse --show-toplevel`

$TOP/run-checks

$TOP/xsum-time 10 1 100000000
$TOP/xsum-time 100 1 10000000
$TOP/xsum-time 1000 1 1000000
$TOP/xsum-time 10000 1 100000
$TOP/xsum-time 100000 1 10000
$TOP/xsum-time 1000000 1 1000
$TOP/xsum-time 10000000 1 100
